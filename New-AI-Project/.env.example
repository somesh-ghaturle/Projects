# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Application Settings
APP_NAME=Multi-Agent AI Research Assistant
APP_VERSION=1.0.0
DEBUG=True
LOG_LEVEL=INFO

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Local LLM Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3

# Agentic AI Framework Settings
CREWAI_MODEL=gpt-4
LANGGRAPH_MODEL=gpt-4
AUTOGEN_MODEL=gpt-4

# Database Configuration
DATABASE_URL=sqlite:///./data/research_db.db
VECTOR_DB_PATH=./data/vector_db
CHROMA_PERSIST_DIR=./data/chroma_db

# Search and Research APIs
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_CSE_ID=your_custom_search_engine_id
BING_SEARCH_API_KEY=your_bing_search_api_key
SERPAPI_API_KEY=your_serpapi_key

# Academic Research APIs
SEMANTIC_SCHOLAR_API_KEY=your_semantic_scholar_key
ARXIV_API_URL=http://export.arxiv.org/api/query
PUBMED_API_KEY=your_pubmed_api_key

# News APIs
NEWS_API_KEY=your_news_api_key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key

# Redis Configuration (for task queues)
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Security
SECRET_KEY=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Performance Settings
MAX_CONCURRENT_AGENTS=5
RESEARCH_TIMEOUT_SECONDS=300
MAX_SOURCES_PER_QUERY=20
WORKFLOW_RETRY_ATTEMPTS=3

# Feature Flags
ENABLE_WEB_SCRAPING=True
ENABLE_ACADEMIC_SEARCH=True
ENABLE_NEWS_SEARCH=True
ENABLE_CREW_AI=True
ENABLE_LANGGRAPH=True
